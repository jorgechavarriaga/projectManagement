<% @projects.each do |project| %>
  <div class="index_row clearfix">
    <h2 class="todo_list_title"><%= link_to project.tittle, project %></h2>
    <p class="todo_list_sub_title"><%= project.description %></p>
    <p class="todo_list_created_at"><%= project.created_at %></p>
    <hr>
  </div>
<% end %>

<div class="links">
  <%= link_to "New Projects", new_project_path, class:"link_to" %>
</div>